{
  "name": "kepler-outdated-luma.gl-scripts",
  "description": "Scripting with luma.gl",
  "license": "MIT",
  "version": "7.3.2",
  "private": "true",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "webgl",
    "visualization",
    "3D"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber/luma.gl"
  },
  "main": "dist/lumagl.min.js",
  "scripts": {
    "start": "webpack-dev-server --env.dev --progress --open",
    "build-dev": "webpack --env.dev --config webpack.config.js",
    "build-prod": "webpack --config webpack.config.js",
    "build": "npm run build-prod"
  },
  "dependencies": {
    "math.gl": "^3.0.0"
  },
  "devDependencies": {
    "kepler-outdated-luma.gl-constants": "file:../constants",
    "kepler-outdated-luma.gl-core": "file:../core",
    "kepler-outdated-luma.gl-debug": "file:../debug",
    "kepler-outdated-luma.gl-glfx": "file:../glfx",
    "kepler-outdated-luma.gl-shadertools": "file:../shadertools"
  },
  "peerDependencies": {
    "kepler-outdated-luma.gl-constants": "https://github.com/EQWorks/luma.gl/raw/build/outdated/modules/constants/dist.tgz",
    "kepler-outdated-luma.gl-core": "https://github.com/EQWorks/luma.gl/raw/build/outdated/modules/core/dist.tgz",
    "kepler-outdated-luma.gl-debug": "https://github.com/EQWorks/luma.gl/raw/build/outdated/modules/debug/dist.tgz",
    "kepler-outdated-luma.gl-glfx": "https://github.com/EQWorks/luma.gl/raw/build/outdated/modules/glfx/dist.tgz",
    "kepler-outdated-luma.gl-shadertools": "https://github.com/EQWorks/luma.gl/raw/build/outdated/modules/shadertools/dist.tgz"
  }
}
